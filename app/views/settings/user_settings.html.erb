<% content_for :setting_data do %>
	<%= @current_full_user.picture_path %>
	<%= form_for(@current_full_user, url: {action: :upload_pictute}) do |f|%>
	  <%= f.file_field :picture %>
	  <%= f.text_area :desc, :class => 'form-control', :id => 'desc-input' %>
	  <%= f.label 'Is Auto Approval : ' %> <%= f.check_box :is_auto_approval %>
	  <br>
	  <%= f.submit 'Submit' %>
	<% end %>
<% end %>
	 
<%= render template: "settings/settings" %>