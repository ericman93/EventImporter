<script>
	var user_mail = "<%= @current_user.email %>"
	var requests = <%= @requests.to_json({:include => :request_proposals}).html_safe %>;
</script>

<div id="requests">
	<b>Requests</b>
    <ul class="nav nav-pills nav-stacked">
        <!--li class="active"><a href="#">Home</a></li-->
        <% @requests.each do |request| %>
            <li id="request_<%= request.id %>">
                <a href="#" onclick="show_request_proposels(<%= request.id %>)"><%= request.subject %> from <%= request.return_name %></a>
            </li>
        <% end %>
    </ul>
</div>

<div id="proposels">
	<b>Proposels</b>
	<ul class="nav nav-pills nav-stacked" id="request_proposels_list">
		
	</ul>
</div>

<div id='calendar'></div>